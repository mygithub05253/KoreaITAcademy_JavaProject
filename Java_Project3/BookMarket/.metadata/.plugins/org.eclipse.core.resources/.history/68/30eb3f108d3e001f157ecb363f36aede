
// 고객 정보 클래스 생성
public class Person {
	private String name;
	private int phone;
	private String address;
	
	public Person(String name, int phone) {
		this.name = name;
		this.phone = phone;
	}
	
	public Person(String name, int phone, String address) {
		this.name = name;
		this.phone = phone;
		this.address = address;
	}
	
	public static void main(String[] args) {
		
	}

}
